<view id="rectificationTaskDetail" class="data-v-3f3e090a"><map style="width:100%;height:50vh;" id="locationMap" controls="{{controls}}" scale="{{18}}" show-location="{{true}}" markers="{{covers}}" circles="{{circles}}" latitude="{{latitude}}" longitude="{{longitude}}" data-event-opts="{{[['controltap',[['bindControltap',['$event']]]]]}}" bindcontroltap="__e" class="data-v-3f3e090a"></map><view class="report_info data-v-3f3e090a"><view class="taskName data-v-3f3e090a">{{"任务名称："+(taskDetail.taskName||"")}}</view><view class="taskName data-v-3f3e090a">{{"问题详情："+(taskDetail.problemDetail||"")}}</view><view class="title data-v-3f3e090a">整改结果描述</view><textarea name placeholder="最多输入200个文字" maxlength="200" id="describe" data-event-opts="{{[['input',[['inpDetail',['$event']]]]]}}" value="{{taskDescription}}" bindinput="__e" class="data-v-3f3e090a"></textarea></view><view class="pictures data-v-3f3e090a"><view data-event-opts="{{[['tap',[['takingPictures']]]]}}" class="takingPictures data-v-3f3e090a" bindtap="__e"><image style="width:46rpx;height:38rpx;" src="/static/images/icon_takePicture.png" mode class="data-v-3f3e090a"></image></view><block wx:for="{{pictures}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="picture_list data-v-3f3e090a"><image src="{{item}}" mode="aspectFill" class="data-v-3f3e090a"></image><image class="del data-v-3f3e090a" src="/static/images/icon_ban.png" mode data-event-opts="{{[['tap',[['delPicture',[index]]]]]}}" bindtap="__e"></image></view></block></view><view class="footer data-v-3f3e090a"><view data-event-opts="{{[['tap',[['cancel']]]]}}" class="cancel data-v-3f3e090a" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['getDistance']]]]}}" class="confirm data-v-3f3e090a" bindtap="__e">完成</view></view></view>